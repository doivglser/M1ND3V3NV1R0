#!/bin/bash
# startup file on /usr/local/bin
# I want to search a new skinn for vlc, but my mom
  
		if [[ $(ps axu | grep -v grep | grep -v hi_vlc | grep 169311-inkyV2) = '' ]] ;
        then
                vlc -I skins2 --skins2-last=/usr/share/vlc/skins2/169311-inkyV2.vlt file:///home/$USER/Music/ & hi ;
        else
                exit 0 ;
fi
