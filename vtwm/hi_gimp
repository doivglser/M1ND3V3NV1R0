#!/bin/bash
# startup file on /usr/local/bin
LANG="C"; 
IFS=$(echo -en "\b\n") ;

		if [[ ! "$(ps aux | grep -v grep | grep -v nano | grep -v geany| grep shi3lD | awk '{print $2}' | wc -l) " -ge "1" ]] && [[ ! $(netstat -ar) =~ 'default' ]] ;
	then
		hi && exit 0 ; 
else
		if [[ $(ps axu | grep -v grep | grep -v nano | grep -v geany | grep -v hi_gimp | grep gimp) = '' ]] ;
        then
                gimp &
                hi && exit 0 ;
        else
                hi && exit 0 ;
fi
fi
