#!/bin/bash
# startup file on /usr/local/bin
#
LANG="C"; 
IFS=$(echo -en "\b\n") ;

			if [[ $(ps axu | grep -v grep | grep -v nano | grep -v geany | grep -v hi_dwecker | grep dwecker) = '' ]] ;
        then
				xterm -title waiker -fg yellow -geometry 60x10 -e dwecker &
                hi && exit 0 ;
        else
                hi && exit 0 ;
fi
